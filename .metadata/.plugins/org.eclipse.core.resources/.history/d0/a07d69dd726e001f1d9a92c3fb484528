package com.example.demo.lombok;

import org.junit.jupiter.api.Test;
import org.springframework.boot.test.context.SpringBootTest;

@SpringBootTest
public class exTest {
	
	@Test
	void 북테스트() {
		Book book1 = new Book();
		book1.title = "가나다1";
		book1.price = 20000;
		book1.compuny = "출판사1";
		book1.page = 120;
		
		Book book2 = new Book("가나다2", 30000, "출판사2", 250);
		
		Book book3 = Book
						.builder()
						.title("가나다3")
						.price(30000)
						.compuny("출판사3")
						.page(200)
						.build();
	}

}
